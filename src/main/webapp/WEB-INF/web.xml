<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>login.LoginPage</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>Loginhtml.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>EnderPage</display-name>
    <servlet-name>EnderPage</servlet-name>
    <servlet-class>login.EnderPage</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EnderPage</servlet-name>
    <url-pattern>/EnderPage</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>PdfPage</display-name>
    <servlet-name>PdfPage</servlet-name>
    <servlet-class>login.PdfPage</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PdfPage</servlet-name>
    <url-pattern>/PdfPage</url-pattern>
  </servlet-mapping>
</web-app>